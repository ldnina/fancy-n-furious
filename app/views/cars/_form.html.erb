<%= simple_form_for(@car) do |f| %>
  <%= f.input :model %>
  <%= f.input :brand %>
  <%= f.input :year %>
  <%= f.input :car_description %>
  <%= f.input :category, collection: %w[F1 Pimp StockCar Fantasy Cartoon LeMans] %>
  <%= f.input :daily_amount %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Save", class: "btn btn-primary" %>
  <%= link_to 'Back', url_for(:back), class: 'btn btn-primary' %>
<% end %>
